{
    "name": "cnrp/laravel-model-crud",
    "description": "A reusable model CRUD component for Laravel Livewire",
    "type": "library",
    "require": {
        "php": "^8.1", 
        "livewire/livewire": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "Cnrp\\ModelCRUD\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Cnrp\\ModelCRUD\\ModelCRUDServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
