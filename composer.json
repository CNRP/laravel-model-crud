{
    "name": "cnrp/laravel-model-crud",
    "description": "A reusable Laravel Livewire component for CRUD operations.",
    "type": "library",
    "require": {
        "php": "^8.1",
        "livewire/livewire": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "Cnrp\\ModelCRUD\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Cnrp\\ModelCRUD\\ModelCRUDServiceProvider"
            ]
        }
    }
}
